{
        "manifest_version": 2,

        "name": "Unicode Obfuscation",
        "description": "This obfucates the text input on Twitter to be in unicode characters that mimic their English counterparts.",
        "version": "0.1",
        "icons": {
                "16": "icons/16.png",
                "48": "icons/48.png",
                "128": "icons/128.png"
        },
        "browser_action": {
                "default_icon": "icons/off.png"
        },
        "background": {
                "scripts": ["js/background.js"]
        },
        "permissions": [
                "activeTab",
                "storage",
                "https://twitter.com/"
        ]
}
